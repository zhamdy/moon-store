{
  "name": "moon-store",
  "private": true,
  "scripts": {
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.56.0"
  },
  "lint-staged": {
    "client/src/**/*.{ts,tsx}": [
